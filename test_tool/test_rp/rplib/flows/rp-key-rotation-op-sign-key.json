{
  "desc": "Support OP Encryption Key Rotation",
  "sequence": [
    {
      "Webfinger": {
        "set_webfinger_resource": null
      }
    },
    {
      "Discovery": {
        "set_discovery_issuer": null
      }
    },
    "Registration",
    "SyncAuthn",
    {
      "AccessToken": {
        "conditional_execution": {
          "profile": "CIT,CI,C,CT"
        }
      }
    },
    {
      "UpdateProviderKeys": {
        "remove_grant": null
      }
    },
    "SyncAuthn",
    {
      "AccessToken": {
        "conditional_execution": {
          "profile": "CIT,CI,C,CT"
        }
      }
    },
    {
      "RestoreKeyJar": {
        "set_op_args": {
          "jwks_path": "static/jwk.json"
        }
      }
    }
  ],
  "usage": {
    "return_type": [
      "C", "I", "IT", "CI", "CT", "CIT"
    ]
  }
}
